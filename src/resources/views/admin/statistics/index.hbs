              <div class="card card-table">
                <div class="card-header">Thống kê
                    <div class="card-subtitle">
                      <div id="sandbox-container" class="col-4">
                      <div class="input-daterange input-group" id="datepicker">
                          <input type="text" onchange="getList()" class="input-sm form-control" id="from" name="start" />
                          <span class="input-group-addon">Đến ngày</span>
                          <input type="text" onchange="getList()" class="input-sm form-control" id="to" name="end" />
                      </div>
                      </div>
                    </div>
                    <div class="dropdown">
                      <button type="button" class="btn btn-light dropdown-toggle" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Lọc chất liệu
                      </button>
                      <div class="dropdown-menu">
                        <span value="Da" class="dropdown-item material">Da</span>
                        <span value="Cao su" class="dropdown-item material">Cao su</span>
                        <span value="Vải" class="dropdown-item material">Vải</span>
                        <!--<a href="?sn=createAt&s=1" class="dropdown-item" id="sort5">Cũ nhất</a>
                        <a href="?sn=createAt&s=-1" class="dropdown-item" id="sort6">Mới nhất</a>
                        <a href="?sn=name&s=1" class="dropdown-item" id="sort7">Bán chạy nhất</a>-->
                      </div>
                    </div><div class="dropdown">
                      <button type="button" class="btn btn-light dropdown-toggle" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Lọc loại giày
                      </button>
                      <div class="dropdown-menu">
                        <span class="dropdown-item sexual">Giày nam</span>
                        <span class="dropdown-item sexual">Giày nữ</span>
                        <!--<a href="?sn=createAt&s=1" class="dropdown-item" id="sort5">Cũ nhất</a>
                        <a href="?sn=createAt&s=-1" class="dropdown-item" id="sort6">Mới nhất</a>
                        <a href="?sn=name&s=1" class="dropdown-item" id="sort7">Bán chạy nhất</a>-->
                      </div>
                    </div>
                </div>
                <div class="card-body">
                  <table class="table table-striped table-hover table-fw-widget" id="table1">
                    <thead>
                      <tr>
                        <th>SKU</th>
                        <th>Tên sản phẩm</th>
                        <th>Size</th>
                        <th>Chất liệu</th>
                        <th>Loại giày</th>
                        <th>Số lượng bán</th>
                        <th>Ngày bán</th>
                        <!-- <th>Danh sách mặt hàng</th>
                        <th>Hình thức thanh toán</th>
                        <th>Tổng giá tiền</th>
                        <th>Trang thái đơn hàng</th> -->
                        {{!-- <th>Thao tác</th> --}}
                      </tr>
                    </thead>
                    <tbody id="list">
                      {{#each data}}
                        <tr class="odd name">
                         <td>{{this.item.SKU}}</td>
                         <td>{{this.item.productName}}</td>
                         <td>{{this.item.size}}</td>
                         <td>{{this.item.material}}</td>
                         <td>{{this.item.sexual}}</td>
                         <td>{{this.qty}}</td>
                         <td>{{this.createdAt}}</td>
                      </tr>
                      
                      {{/each}}
                      {{!-- <tr class="odd name">
