<div class="be-content">
        <div class="page-head">
          <h2 class="page-head-title">Danh sách nhập hàng</h2>
          <nav aria-label="breadcrumb" role="navigation">
            <ol class="breadcrumb page-head-nav">
              <li class="breadcrumb-item"><a href="/admin">Trang chủ</a></li>
              <li class="breadcrumb-item"><a href="/admin/stockin">Danh sách nhập hàng</a></li>
              <li class="breadcrumb-item active">Danh sách nhập hàng</li> 
            </ol>
          </nav>
        </div>
        <div class="main-content container-fluid">
          <div class="row">
            <div class="col-sm-12">
              <div class="card card-table">
                <div class="card-header">Nhập hàng
                  <div class="float-lg-right">
                    <a href="/admin/stockin/addstockin"><button class="btn btn-space btn-success hover">Nhập hàng</button></a>
                  </div>
                </div>
                <div class="card-body">
                  <table class="table table-striped table-hover table-fw-widget" id="table1">
                    <thead>
                      <tr>
                        <th>STT</th>
                        <th>Tên danh sách</th>
                        <th>Người nhập</th>
                        <th>Ngày nhập</th>
                        <th>Thao tác</th>
                      </tr>
                    </thead>
                    <tbody>
                      {{#each list}}
                      
                      <tr class="odd name">
                        <td>{{sum @index 1}}</td>
                        <td>
                          {{this.file.originalname}}
                        </td>
                        <td>
                          {{this.adminId.fullname}}
                        </td>
                        <td>{{this.createdAt}}</td>
                        <td class="center">
                          <a href="/admin/stockin/{{this._id}}"><button class="btn btn-rounded btn-space btn-primary">Chi tiết</button></a>
                        </td>
                      </tr>
                      {{/each}}
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
